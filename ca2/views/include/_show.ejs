<% for(var i = 0; i < users.length; i++) { %>
<tr>
    <td> <%= i+1> </td>
    <td> <%= users[i].name %> </td>
    <td> <%= users[i].type %> </td>
    <td> <%= users[i].price %> </td>
    <td>
        <a href="/update-dish" class="btn border-shadow update">
            <span class="text-gradient">Edit</span>
        </a>
        <a  href="#" class="btn border-shadow delete"> >
            <span class="text-gradient">Delete</span>
        </a>
    </td>
</tr>
<% } %>